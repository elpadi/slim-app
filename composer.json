{
	"name": "elpadi/slim-app",
	"type": "library",
	"description": "Starting point for a website using slim, propel, and twig.",
	"keywords": ["website","slim","propel","twig"],
	"homepage": "http://github.com/elpadi/slim-app.git",
	"license": "MIT",
	"authors": [
		{
			"name": "elpadi",
			"email": "padi_05@yahoo.com",
			"homepage": "http://github.com/elpadi"
		}
	],
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/elpadi/php-libs.git"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.3.0",
		"lstrojny/functional-php": "~1.0",
		"slim/slim": "^3.0",
		"twig/twig": "~1.0",
		"symfony/config": "~3.0",
		"symfony/finder": "~3.0",
		"hassankhan/config": "~0.10",
		"propel/propel": "~2.0@dev"
	},
	"autoload": {
		"psr-4": {
			"App\\": "src"
		},
		"classmap": ["src/Propel/"]
	}
}
